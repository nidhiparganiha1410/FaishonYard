{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/go/:slug",
            "destination": "/api/go/[slug]"
        },
        {
            "source": "/((?!api|go/).*)",
            "destination": "/index.html"
        }
    ]
}